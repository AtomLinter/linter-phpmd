{
  "name": "linter-phpmd",
  "main": "./lib/main",
  "version": "1.3.2",
  "description": "Lint PHP on the fly, using phpmd",
  "repository": "https://github.com/AtomLinter/linter-phpmd",
  "license": "MIT",
  "dependencies": {
    "atom-linter": "^3.3.2",
    "atom-package-deps": "^3.0.1"
  },
  "devDependencies": {
    "coffeelint": "^1.13.1"
  },
  "package-deps": [
    "linter"
  ],
  "scripts": {
    "lint": "./node_modules/.bin/coffeelint lib"
  },
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  }
}
