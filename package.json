{
  "name": "linter-phpmd",
  "main": "./lib/main",
  "version": "1.2.1",
  "description": "Lint PHP on the fly, using phpmd",
  "repository": "https://github.com/AtomLinter/linter-phpmd",
  "license": "MIT",
  "dependencies": {
    "atom-linter": "^3.2.1",
    "atom-package-deps": "^2.0.5"
  },
  "devDependencies": {
    "coffeelint": "^1.12.1"
  },
  "package-deps": [
    "linter"
  ],
  "scripts": {
    "lint": "./node_modules/.bin/coffeelint lib"
  },
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  }
}
